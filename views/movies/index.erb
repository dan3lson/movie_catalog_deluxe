<h1>
  Movies
</h1>

<table>
  <th>
    Movie
  </th>
  <th>
    Year
  </th>
  <th>
    Rating
  </th>
  <th>
    Genre
  </th>
  <th>
    Studio
  </th>

<% movies.each do |movie| %>
  <tr>
    <td>
      <a href="/movies/<%= movie["id"] %>">
        <%= movie["title"] %>
      </a>
    </td>
    <td>
      <%= movie["year"] %>
    </td>
    <td>
      <%= movie["rating"] %>
    </td>
    <td>
      <%= movie["genre"] %>
    </td>
    <td>
      <%= movie["studio"] %>
    </td>
  </tr>
<% end %>
</table>

